{
  "detection": {
    "confidence_threshold": 0.5,
    "model_path": "app/models/yolo11m_best.pt",
    "model_device": "auto",
    "processing_frequency": 3,
    "detection_timeout": 30,
    "max_detections": 10,
    "enhancement_enabled": true,
